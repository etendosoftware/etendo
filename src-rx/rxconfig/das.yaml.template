server:
  port: 8092

spring:
  datasource:
    url: jdbc:postgresql://localhost:5470/etendo
    username: tad
    password: tad
  jackson:
    serialization:
      FAIL_ON_EMPTY_BEANS: false
  jpa:
    show-sql: true
    properties:
      hibernate:
        format_sql: true

logging:
  level:
    web: # Shows each request on output
      INFO
    org:
      hibernate:
        type:
          descriptor: # Shows each hibernate SQL query
            sql: WARN
